<?xml version="1.0" encoding="UTF-8"?>
<ruleset name="Generic - VersionControl"
         xmlns="http://pmd.sf.net/ruleset/1.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://pmd.sf.net/ruleset/1.0.0 http://pmd.sf.net/ruleset_xml_schema.xsd"
         xsi:noNamespaceSchemaLocation="http://pmd.sf.net/ruleset_xml_schema.xsd">
  <description>
Sniffs related to version control
  </description>
  <rule name="SubversionProperties" class="Generic_Sniffs_VersionControl_SubversionPropertiesSniff">
    <description>
Ensures expected subversion properties exist on files.

By default the expected properties are svn:keywords and svn:eol-style.
    </description>
  </rule>
</ruleset>
